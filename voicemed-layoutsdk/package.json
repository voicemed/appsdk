{
  "name": "Airlyn",
  "version": "1.0.20",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt",
    "devprod": "NODE_OPTIONS=\"--max-old-space-size=8000\" BUILD_ENV=production nuxt",
    "develop": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=develop BUILD_ENV=staging nuxt build",
    "staging": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=staging BUILD_ENV=staging nuxt build",
    "stagingtest": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=staging BUILD_ENV=staging nuxt --hostname 0.0.0.0 --port 3000",
    "productiontest": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=production BUILD_ENV=production nuxt",
    "voicemed_stagingtest": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=voicemed_staging BUILD_ENV=voicemed_staging nuxt --hostname 0.0.0.0 --port 3000",
    "voicemed_productiontest": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=voicemed_production BUILD_ENV=voicemed_production nuxt",
    "production": "nuxt build",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && npm run copy",
    "publish": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=production BUILD_ENV=production nuxt build && NODE_ENV=production BUILD_ENV=production nuxt generate && npm run copy",
    "publishcap": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=production BUILD_ENV=production nuxt generate && npx cap sync && npm run copy",
    "voicemed_publishcap": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=voicemed_production BUILD_ENV=voicemed_production nuxt generate && npx cap sync && npm run copy",
    "publishstagingcap": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=staging BUILD_ENV=staging nuxt generate && npx cap sync && npm run copy",
    "voicemed_publishstagingcap": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=voicemed_staging BUILD_ENV=voicemed_staging nuxt generate && npx cap sync && npm run copy",
    "generate-capacitor": "npm run publish && npx cap copy",
    "generate-cap": "npm run publishcap && npx cap copy",
    "voicemed_generate-cap": "npm run voicemed_publishcap && npx cap copy",
    "voicemed_generate-lib-cap": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=staging BUILD_ENV=library nuxt generate",
    "voicemed_generate-lib-dev": "NODE_OPTIONS=\"--max-old-space-size=8000\" NODE_ENV=staging BUILD_ENV=library-dev nuxt generate",
    "copy": "cp proxy.php ./dist/",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "lintfix": "npm run lint:js -- --fix",
    "test": "jest"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^5.2.0",
    "@foobar404/wave": "^2.0.5",
    "@nuxtjs/auth-next": "^5.0.0-1667386184.dfbbb54",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/i18n": "^7.3.1",
    "@nuxtjs/proxy": "^2.1.0",
    "core-js": "^3.25.3",
    "csv": "^6.3.6",
    "device-detector-js": "^3.0.3",
    "flotruglio-weve": "file:weveFork",
    "humanize-duration": "^3.31.0",
    "lru-cache": "^7.18.3",
    "nuxt": "^2.15.8",
    "nuxt-vuex-localstorage": "^1.3.0",
    "object-hash": "^3.0.0",
    "sanitize-html": "^2.11.0",
    "superapi-cache": "^0.12.0",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vuetify": "^2.6.10",
    "worker-loader": "^3.0.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxt/bridge": "^3.0.1",
    "@nuxt/types": "^2.17.3",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/device": "^2.3.0",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/svg": "^0.4.1",
    "@nuxtjs/vuetify": "^1.12.3",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.1.2",
    "eslint": "^8.24.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "nuxt-animejs": "^2.0.0",
    "nuxt-compress": "^5.0.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.16"
  }
}
